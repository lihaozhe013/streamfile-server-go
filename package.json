{
  "name": "simple-server-go",
  "version": "1.0.0",
  "description": "A lightweight file server written in Go",
  "main": "index.js",
  "scripts": {
    "build:all": "./build.sh",
    "build": "npm run build-css-prod",
    "build-css": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./public/output.css --minify"
  },
  "keywords": [
    "go",
    "file-server",
    "tailwindcss"
  ],
  "author": "lihaozhe013",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  }
}
